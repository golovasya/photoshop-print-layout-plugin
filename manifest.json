{
  "manifestVersion": 5,
  "id": "com.golovasya.printlayout",
  "name": "Print Layout Manager",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0",
      "data": {
        "apiVersion": 2
      }
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "printlayout",
      "label": {
        "default": "Print Layout Manager"
      },
      "minimumSize": {
        "width": 350,
        "height": 500
      },
      "maximumSize": {
        "width": 800,
        "height": 1200
      },
      "preferredDockedSize": {
        "width": 400,
        "height": 700
      },
      "preferredFloatingSize": {
        "width": 450,
        "height": 750
      }
    }
  ],
  "requiredPermissions": [
    "localFileSystem",
    "launchProcess",
    "network",
    "enableAddon",
    "allowCodeGenerationFromStrings"
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/icon.png"
    }
  ]
}
