{
  "id": "com.golovasya.printlayout",
  "name": "Print Layout Manager",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "24.0.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "printLayoutPanel",
      "label": {
        "default": "Print Layout Manager"
      },
      "minimumSize": {
        "width": 350,
        "height": 500
      },
      "maximumSize": {
        "width": 800,
        "height": 1200
      },
      "preferredDockedSize": {
        "width": 400,
        "height": 600
      },
      "preferredFloatingSize": {
        "width": 450,
        "height": 700
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/icon-light.png",
      "scale": [1, 2],
      "theme": ["lightest", "light", "medium"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/icon-dark.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark"]
    }
  ],
  "requiredPermissions": [
    "localFileSystem",
    "launchProcess",
    "network",
    "allowCodeGenerationFromStrings"
  ]
}